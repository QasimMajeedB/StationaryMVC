@model IEnumerable<Stationary.Models.question_answers>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.messages)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.froms)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tos)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dates)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.messages)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.froms)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tos)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dates)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
